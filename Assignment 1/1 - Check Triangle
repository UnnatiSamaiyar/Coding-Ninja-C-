//print 1: if triangle is equilateral
//print 0: if triangle is isosceles
//print -1: if triangle is scalene

#include<bits/stdc++.h>
using namespace std;

int main(){
	int a;
	int b;
	int c;
	cin>>a>>b>>c;
	cout<<"a:"<<a<<" b:"<<b<<" c:"<<c<<endl;;
	if(a==b && b==c){
		cout<<"1";
	}
	else if(a==b && b!=c || a!=b && b==c){
		cout<<"0";
	}
	else{
		cout<<"-1";
	}
}
