#include<bits/stdc++.h>
using namespace std;

int main(){
    int x;
    cin>>x;
    int i=1, count =1;
    while(count<=x){
        if((3*i+2)%4==0){
            i++;
            continue;
        }
        else{
            cout<<(3*i+2)<<" ";
            i++;
            count++;
        }
    }
}
